<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>OnlineStore2020</display-name>
  
  <servlet>
  	<servlet-name>logincontroller</servlet-name>
  	<servlet-class>ph.com.cpi.onlinestore2020.controller.LoginController</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>logincontroller</servlet-name>
  	<url-pattern>/user-login</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>registrationcontroller</servlet-name>
  	<servlet-class>ph.com.cpi.onlinestore2020.controller.RegistrationController</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>registrationcontroller</servlet-name>
  	<url-pattern>/user-register</url-pattern>
  </servlet-mapping>
  
  
  <welcome-file-list>
    <welcome-file>user-login</welcome-file>
  </welcome-file-list>
</web-app>